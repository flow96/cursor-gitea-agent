{
    "mcpServers": {
        "gitea": {
          "command": "${env:GITEA_MCP_PATH}",
          "args": [
            "-t",
            "stdio"
          ],
          "env": {
            "GITEA_HOST": "${env:GITEA_BASE_URL}",
            "GITEA_ACCESS_TOKEN": "${env:GITEA_ACCESS_TOKEN}"
          }
      }
    }
  }
  